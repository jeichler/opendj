apiVersion: v1
kind: ConfigMap
metadata:
    name: frontend-web-config-map
    namespace: "{{ENV.COMMON.NAMESPACE|mandatory}}"
data:
    config.json: |-
        {
        "enableDebug": true,
        "curatorPassword": "test",
        "playlistMaxSize": 50,
        "websocketHost": "{{ENV.FRONTEND_WEV.WEBSOCKET_HOST | default('')}}" ,
        "websocketPath": "/api/service-web/socket.io",
        "SPOTIFY_PROVIDER_API": "/api/provider-spotify/v1" ,
        "PLAYLIST_PROVIDER_API": "/api/service-playlist/v1" 
        }
